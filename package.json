{
  "name": "life-book-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "cd LifeBookProject/LifeBookProject && mvnw spring-boot:run && cd frontend && react-scripts start",
    "build": "cd LifeBookProject/LifeBookProject && mvnw install && cd frontend && react-scripts build",
    "test": "cd LifeBookProject/LifeBookProject/frontend && react-scripts test",
    "eject": "cd LifeBookProject/LifeBookProject/frontend && react-scripts eject"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}